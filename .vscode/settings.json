{
    "editor.wordWrap": "off",
    "files.exclude": {
			"**/.vscode": {},
			"**/node_modules": {},
			"typings": {},
			"**/*.map": {},
			"**/*.js": {
				"when": "$(basename).ts"
			}
	},
  "files.autoSave": "onWindowChange",
	"editor.fontFamily": "'Courier New', Menlo, Monaco, monospace",
  "tslint.enable": true,
  "sonarlint.ls.javaHome": "/usr/jdk-10.0.1/"
}
